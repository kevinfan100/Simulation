        <P Name="isEML">1</P>
        <P Name="script">function vd = fcn(signal_type_in, channel_in, amplitude_in, frequency_in, phase_in, step_time_in, t)

    % 固定參數
    Ts = 1e-5;  

    % Preview 機制
    t_future = t + Ts * 3;

    % 初始化輸出
    vd = zeros(6, 1);

    % 確保 channel 在有效範圍內
    channel = round(channel_in);
    if channel &lt; 1 || channel &gt; 6
        channel = 1;
    end

    % 根據 signal_type 生成信號
    if signal_type_in == 1
        % Sine Wave
        vd(channel) = amplitude_in * sin(2*pi*frequency_in*t_future + 180 * phase_in /  pi);
    else
        % Step 模式
        if t_future &gt;= step_time_in
            vd(channel) = amplitude_in;
        else
            vd(channel) = 0;
        end
    end
end
</P>
      </eml>
